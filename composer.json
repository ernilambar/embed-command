{
    "name": "wp-cli/embeds-command",
    "description": "Fetches and inspectes embeds",
    "type": "wp-cli-package",
    "homepage": "https://github.com/wp-cli/embeds-command",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "embeds-command.php"
        ],
        "psr-4": {
            "WP_CLI\\Embeds\\": "src/"
        }
    },
    "require": {
        "wp-cli/wp-cli": "^1.1.0"
    },
    "require-dev": {
        "behat/behat": "~2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "commands": [
            "embed fetch",
            "embed provider list",
            "embed provider get",
            "embed handler list",
            "embed cache clear",
            "embed cache find",
            "embed cache trigger"
        ],
        "bundled": true
    }
}
